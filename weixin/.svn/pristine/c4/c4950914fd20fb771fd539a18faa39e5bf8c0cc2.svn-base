<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1" />
<title>vocationapply</title>
<link rel="stylesheet" href="../css/jquery.mobile-1.4.5.min.css" />
<script src="../js/jquery-1.11.1.min.js"></script>
<script src="../js/jquery.mobile-1.4.5.min.js"></script>

<!--Start 使用日期控件 引入文件-->
<script src="../js/jqm-datebox-1.4.5.js" type="text/javascript"></script>
<link href="../css/jqm-datebox-1.4.5.css" rel="stylesheet" type="text/css" />
<!--End 使用日期控件 引入文件-->
</head>

<script type="text/javascript">
$(document).on("pageinit","#div_vocationapply",function(){
	//获取当前日期
	var d = new Date();
	var vYear = d.getFullYear();
	var vMonth = d.getMonth()+1;
	var vDay = d.getDate();
	
	var vdate = vMonth+"/"+vDay+"/"+vYear;
	$("#txtStartDate").val(vdate);

	//返回首页
	$("#btnindex").on("click",function(){
		window.location.href="index.html";
	});

}); 
</script>


<body>
<div data-role="page" id="div_vocationapply" data-add-back-btn="true">
	<!-- 页眉 -->
	<div data-role="header"><!--data-position="fixed"-->
		<a id="btnindex" data-role="button" data-icon="home">首页</a>
		<h1>请假申请</h1>
	</div>
	
	
	<div data-role="content"><!-- 内容 START -->
	 <div data-role="fieldcontain">
		<label for="lbltype">假别：</label>
		<select name="seltype" id="seltype" data-inline="true">
			<option value="1" selected>带薪假</option>
			<option value="2">病假</option>
			<option value="3">事假</option>
			<option value="4">婚假</option>
			<option value="5">丧假</option>
			<option value="6">调休</option>
			<option value="7">产假</option>
			<option value="8">陪产假</option>
		</select>
	   </div>

	   <div data-role="fieldcontain">
		<label for="lblStartDate">请假时间：</label>
		<input type="text" name="txtStartDate" id="txtStartDate" readonly="true" data-role="datebox" data-options='{"mode":"calbox"}' />
	   </div>

	   <div data-role="fieldcontain">
		<label for="lblStartDate">事由：</label>
		<textarea name="txtremark" id="txtremark"></textarea>
	   </div>
		
	</div><!-- 内容 END -->

	

	<!-- 页脚-->
	<div data-role="footer" data-position="fixed">
		<h1>@版权所有&nbsp;&nbsp;&nbsp;上海微恩索软件技术有限公司</h1>
	</div>
</div>



</body>
</html>
